<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service id="nucleos_antispam.form.extension.type.honeypot" class="Nucleos\AntiSpamBundle\Form\Extension\HoneypotFormExtension">
      <tag name="form.type_extension" extended-type="Symfony\Component\Form\Extension\Core\Type\FormType"/>
      <argument type="service" id="translator"/>
      <argument type="collection"/>
    </service>
    <service id="nucleos_antispam.provider.session" class="Nucleos\AntiSpamBundle\Provider\SessionTimeProvider">
      <argument type="service" id="session"/>
    </service>
    <service id="nucleos_antispam.form.extension.type.time" class="Nucleos\AntiSpamBundle\Form\Extension\TimeFormExtension">
      <tag name="form.type_extension" extended-type="Symfony\Component\Form\Extension\Core\Type\FormType"/>
      <argument type="service" id="nucleos_antispam.provider"/>
      <argument type="service" id="translator"/>
      <argument type="collection"/>
    </service>
    <service id="nucleos_antispam.twig.string_extension" class="Nucleos\AntiSpamBundle\Twig\Extension\StringTwigExtension">
      <tag name="twig.extension"/>
      <argument>%nucleos_antispam.twig.mail_css_class%</argument>
      <argument>%nucleos_antispam.twig.mail_at_text%</argument>
      <argument>%nucleos_antispam.twig.mail_dot_text%</argument>
    </service>
  </services>
</container>
